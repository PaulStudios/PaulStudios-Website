    {
      "name": "Run: Django",
      "type": "debugpy",
      "request": "launch",
      "args": ["runserver_plus"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}\\manage.py"
    }
  ]
}
