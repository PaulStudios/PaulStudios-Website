  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test: Django",
      "type": "debugpy",
      "request": "launch",
      "args": ["test --verbosity 2"],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}\\manage.py"
